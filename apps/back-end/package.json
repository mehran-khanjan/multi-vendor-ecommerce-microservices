{
  "name": "back-end",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/root-service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:unit": "jest --testPathPattern=\\.spec\\.ts$ --config jest.config.js",
    "test:integration": "jest --testPathPattern=\\.integration\\.spec\\.ts$ --config jest.config.js",
    "test:e2e": "jest --apps/root-service/testPathPattern=\\.e2e-spec\\.ts$ --config jest.config.js",
    "test:contract": "jest --testPathPattern=contract --config jest.config.js",
    "test:performance": "jest --testPathPattern=performance --config jest.config.js --testTimeout=120000",
    "test:ci": "npm run test:cov && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
